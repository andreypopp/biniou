{
    "name": "biniou",
    "version": "1.0.12",
    "dependencies": {
        "easy-format": "andreypopp/easy-format#esy",
        "ocamlfind": "andreypopp/ocamlfind#esy",
        "conf-which": "andreypopp/conf-which#esy",
        "opam-installer-bin": "andreypopp/opam-installer-bin"
    },
    "esy": {
        "build": [
            "make",
            "make install BINDIR=$cur__bin",
            "(opam-installer --prefix=$cur__install || true)"
        ],
        "exportedEnv": {
            "biniou_enable": {
                "scope": "global",
                "val": "enable"
            },
            "biniou_version": {
                "scope": "global",
                "val": "1.0.12"
            },
            "biniou_installed": {
                "scope": "global",
                "val": "true"
            }
        }
    },
    "peerDependencies": {
        "ocaml": "andreypopp/ocaml#esy"
    }
}
